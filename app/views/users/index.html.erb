<h1>サービス利用者一覧</h1>


<p>検索</p>
  <%= form_tag(users_path,:method => 'get') do %>
  <%= text_field_tag :search %>
  <%= submit_tag 'Search', :name => nil %>
<% end %>

<ul>
  <% @users.each do |user| %>
	  <li>
	    <p>メールアドレス</p>
		<%= link_to user.email, user_path(user.id) %>
		<p> 郵便番号</p>
		<%= user.zip_code %>
		<p> 郵便番号</p>
		<%= user.home_address %>
	  </li>
  <% end %>
  <%= paginate @users, class: "paginate" %>
</ul>
